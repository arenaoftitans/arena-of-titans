    },
    "scripts": {
        "build:dev": "nps clean 'build.config --env dev' 'webpack.build --env.environment=dev'",
        "lint": "nps lint",
        "start": "nps clean 'build.config --env dev' webpack.devserver",
        "test": "nps test",
        "translate": "nps update.translations"
    },
    "version": "0.5.0"
